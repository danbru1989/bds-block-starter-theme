{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"customTemplates": [
		[]
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#3c96d2",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#eb7d3c",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "rgb(131, 171, 197)",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#f0f0f0",
					"name": "Light Grey",
					"slug": "light-grey"
				},
				{
					"color": "#303030",
					"name": "Black",
					"slug": "black"
				}
			]
		},
		"custom": {
			"transition": {
				"normal": "all 0.2s ease-in-out"
			}
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"name": "Small",
					"shadow": "0 2px 3px rgba(0,0,0,.6)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"shadow": "0 5px 10px rgba(0,0,0,.5)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"shadow": "0 10px 20px rgba(0,0,0,.5)",
					"slug": "large"
				}
			]
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Fluid 1",
					"size": "clamp(.5rem, 2vw, 1rem)",
					"slug": "1"
				},
				{
					"name": "Fluid 2",
					"size": "clamp(1rem, 3vw, 2rem)",
					"slug": "2"
				},
				{
					"name": "Fluid 3",
					"size": "clamp(1rem, 4vw, 4rem)",
					"slug": "3"
				},
				{
					"name": "Fluid 4",
					"size": "clamp(1rem, 6vw, 6rem)",
					"slug": "4"
				},
				{
					"name": "Fluid 5",
					"size": "clamp(2rem, 8vw, 8rem)",
					"slug": "5"
				},
				{
					"name": "Fluid 6",
					"size": "clamp(2rem, 10vw, 10rem)",
					"slug": "6"
				},
				{
					"name": "Fluid 7",
					"size": "clamp(2rem, 12vw, 12rem)",
					"slug": "7"
				}
			],
			"units": [
				"%",
				"px",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontWeight": "300 700",
							"src": [
								"file:./assets/fonts/open-sans/OpenSans-VariableFont_wdth,wght.ttf"
							]
						}
					],
					"fontFamily": "\"Open Sans\", sans-serif",
					"name": "Open Sans",
					"slug": "primary"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Roboto Slab",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/roboto-slab/RobotoSlab-VariableFont_wght.ttf"
							]
						}
					],
					"fontFamily": "\"Roboto Slab\", serif",
					"name": "Roboto Slab",
					"slug": "secondary"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "1rem",
						"min": ".875rem"
					},
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Normal",
					"size": "1.25rem",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "1.4rem",
						"min": "1.25rem"
					},
					"name": "Medium",
					"size": "1.3rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.25rem"
					},
					"name": "Large",
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2.5rem",
						"min": "1.5rem"
					},
					"name": "Extra Large",
					"size": "2rem",
					"slug": "extra-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2rem"
					},
					"name": "Huge",
					"size": "2.5rem",
					"slug": "huge"
				},
				{
					"fluid": {
						"max": "4rem",
						"min": "2rem"
					},
					"name": "Gigantic",
					"size": "3rem",
					"slug": "gigantic"
				},
				{
					"fluid": {
						"max": "6rem",
						"min": "3rem"
					},
					"name": "Colossal",
					"size": "4rem",
					"slug": "colossal"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/image": {
				"css": "& figcaption{text-align: center;}",
				"elements": {
					"caption": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)"
						}
					}
				}
			},
			"core/navigation": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--black)"
		},
		"css": "/***** Fix Page Content Headings *****/\n.is-layout-flow * + :is( h1, h2, h3, h4, h5, h6 ),\n.wp-block-post-content * + :is( h1, h2, h3, h4, h5, h6 )  {\n\tmargin-top: var(--wp--preset--spacing--2);\n}\n\n.is-layout-flow :is( h1, h2, h3, h4, h5, h6 ) + *,\n.wp-block-post-content :is( h1, h2, h3, h4, h5, h6 ) + * {\n\tmargin-top: 0;\n}\n\n/***** Site Footer *****/\nfooter.wp-block-template-part {margin-top: 0;}\n\n\n/***** Media Queries *****/\n@media only screen and (max-width: 320px) {\n}\n\n@media only screen and (max-width: 600px) {\n}",
		"elements": {
			"button": {
				":active": {
					"color": {
						"background": "var(--wp--preset--color--primary)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--primary)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)"
					}
				},
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--white)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--gigantic)",
					"lineHeight": "1.75"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "1.75"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--extra-large)",
					"lineHeight": "1.75"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.75"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--secondary)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "#edac87"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--2)",
			"padding": {
				"bottom": "0",
				"left": "var(--wp--preset--spacing--2)",
				"right": "var(--wp--preset--spacing--2)",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "1.5"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}